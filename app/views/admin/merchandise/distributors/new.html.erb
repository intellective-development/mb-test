<% content_for :title, 'Create Distributor' %>
<%= render :partial => '/admin/merchandise/sub_header'%>
<h3>New Distributor</h3>

<%= form_for(@distributor, :url => admin_merchandise_distributors_path(@distributor)) do |f| %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <%= f.submit 'Create', :class => 'button' %>
<% end %>

<p>
  <%= link_to 'Back to List', admin_merchandise_distributors_path, :class => 'button' %>
</p>
