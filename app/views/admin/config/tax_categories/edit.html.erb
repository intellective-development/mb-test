<h3>Editing Tax Category <%= @tax_category.name %> </h3>

<%= simple_form_for(@tax_category, :url => admin_config_tax_category_path(@tax_category)) do |f| %>
  <%= render :partial => 'form', :locals => {:f => f} %>
<% end %>

<%= link_to 'Show', admin_config_tax_category_path(@tax_category), :class => 'button' %>
<%= link_to 'Back', admin_config_tax_categories_path, :class => 'button' %>
