<h3>Editing Page:  <%= @region.name %> </h3>

<%= form_for(@region, url: admin_config_region_page_path(@region)) do |f| %>
  <%= render partial: 'form', locals: { f: f} %>
  <%= f.submit "Update", class: "button" %>
<% end %>

<%= link_to 'Back', admin_config_region_pages_path, :class => 'button' %>
