{
  "a": {
    "fontSize": 12,
    "lineHeight": 1.5,
    "padding": "20px 10px",
    "textAlign": "center"
  },
  "b": {
    "fontWeight": 700,
    "letterSpacing": 1,
    "padding": 5,
    "textTransform": "uppercase"
  },
  "c": {
    "padding": 5
