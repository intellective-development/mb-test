  "i": {
    "alignItems": "center",
    "backgroundColor": "#fff",
    "border": "1px solid rgba(0,0,0,.1)",
    "borderRadius": 3,
    "display": "flex",
    "flexGrow": 1,
    "margin": 5
  },
  "j": {
    "%passwordinput": true,
    "%passwordinput[type=password], %passwordinput[type=text]": {
      "border": "0 none transparent",
      "fontSize": 12,
      "margin": 0,
      "padding": 10
    },
    "::placeholder": {
      "color": "rgba(0,0,0,.5)",
      "fontStyle": "italic",
      "textTransform": "uppercase"
    },
    "boxSizing": "border-box",
    "flexGrow": 1,
    "fontSize": 12,
    "padding": 10
  },
  "k": {
    "::before": {
      "height": 26,
      "margin": "5px 11px",
      "width": 26
    },
    "backgroundColor": "rgba(0,0,0,.1)",
    "borderRadius": "0 2px 2px 0",
    "cursor": "pointer",
    "height": 36
  },
  "l": {
    "backgroundColor": "#781214",
    "border": "1px solid #781214",
    "borderRadius": 5,
    "color": "#fff",
    "cursor": "pointer",
    "fontSize": 10,
    "fontWeight": 700,
    "margin": 5,
    "padding": "11px 22px",
    "textTransform": "uppercase",
    "whiteSpace": "nowrap"
  },
  "m": {
    "@media (max-width: 767px)": {
      "flexDirection": "column"
    },
    "@media (min-width: 768px)": {
      "flexDirection": "row"
    },
    "display": "flex"
  },
  "n": {
    "::before": {
      "height": 20,
      "marginRight": 8,
      "width": 20
