    "alignItems": "center",
    "display": "flex",
    "flexBasis": "50%",
    "fontSize": 12,
    "justifyContent": "center",
    "padding": 15,
    "textAlign": "center"
  },
  "o": {
    "@media (max-width: 767px)": {
      "borderTop": "1px solid rgba(0,0,0,.1)"
    },
    "@media (min-width: 768px)": {
      "borderLeft": "1px solid rgba(0,0,0,.1)"
    }
  },
  "p": {
    "backgroundColor": "#fff",
    "border": "1px solid rgba(0,0,0,.1)",
    "margin": "15px auto",
    "maxWidth": 500
  },
  "q": {
    "@media (max-width: 767px)": {
      "flexDirection": "column"
    },
    "@media (min-width: 768px)": {
      "flexDirection": "row"
    },
    "borderTop": "1px solid rgba(0,0,0,.1)",
    "display": "flex"
  },
  "r": {
    "fontSize": 12,
    "padding": "20px 10px",
    "textAlign": "center"
  },
  "s": {
    "fontSize": 12,
    "backgroundColor": "rgba(120,18,20,.03)",
    "color": "#757575",
    "margin": 5,
    "padding": 10,
    "width": "100%"
  }
