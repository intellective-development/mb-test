    "::before": {
      "backgroundColor": "#e18c63"
    }
  },
  "mixers": {
    "::before": {
      "backgroundColor": "#68ae59"
    }
  },
  "name": {
    "fontSize": 15,
    "fontWeight": 700,
    "marginBottom": 5
